        <div class="pages">
            <ul class="pagination center">
                <li class="waves-effect"><a href="/products/printers"><span class="mdi mdi-chevron-left"></span></a></li>
                <li class="waves-effect"><a href="/products">1</a></li>
                <li class="waves-effect"><a href="/products/systemUnits">2</a></li>
                <li class="waves-effect"><a href="/products/tabs">3</a></li>
                <li class="waves-effect"><a href="/products/screens">4</a></li>
                <li class="waves-effect"><a href="/products/printers">5</a></li>
                <li class="active waves-effect"><a href="/products/accessories">6</a></li>
                <li class="disabled waves-effect"><a><span class="mdi mdi-chevron-right"></span></a></li>
